{
  "name": "mysql",
  "displayName": "MySQL",
  "description": "MySQL language support",
  "version": "0.0.5",
  "publisher": "zendobk",
  "author": {
    "name": "Dao Nguyen",
    "email": "zendobk@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:zendobk/vscode-mysql.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "scripts": {
    "build": "npx vsce package"
  },
  "contributes": {
    "languages": [
      {
        "id": "mysql",
        "aliases": [
          "MySQL",
          "mysql"
        ],
        "extensions": [
          ".sql"
        ],
        "configuration": "./languages/mysql.language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "mysql",
        "scopeName": "source.sql",
        "path": "./syntaxes/mysql.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "mysql",
        "path": "./snippets/mysql.code-snippets.json"
      }
    ],
    "configurationDefaults": {
      "editor.fontSize": 12,
      "editor.tokenColorCustomizations": {
        "textMateRules": [
          {
            "scope": "keyword.control.sql, keyword.operator.function.sql",
            "settings": {
              "foreground": "#569CD6"
            }
          },
          {
            "scope": "support.function.sql, support.function.temporal.sql",
            "settings": {
              "foreground": "#DCDCAA"
            }
          },
          {
            "scope": "string.quoted.single.sql",
            "settings": {
              "foreground": "#CE9178"
            }
          },
          {
            "scope": "string.quoted.double.sql, string.quoted.multi-line.sql, string.quoted.backtick.sql",
            "settings": {
              "foreground": "#CE9178"
            }
          },
          {
            "scope": "constant.numeric.sql",
            "settings": {
              "foreground": "#B5CEA8"
            }
          },
          {
            "scope": "comment.line.double-dash.sql, comment.block.sql, comment.line.hash.sql, comment.conditional.sql",
            "settings": {
              "foreground": "#6A9955"
            }
          },
          {
            "scope": "operator.symbol.sql, operator.symbol.advanced.sql",
            "settings": {
              "foreground": "#D4D4D4"
            }
          },
          {
            "scope": "operator.keyword.sql",
            "settings": {
              "foreground": "#9CDCFE"
            }
          },
          {
            "scope": "entity.name.function.sql, entity.name.table.sql, entity.name.column.sql, variable.global.sql, variable.session.sql, variable.other.sql, variable.user-defined.sql",
            "settings": {
              "foreground": "#D4D4D4"
            }
          },
          {
            "scope": "storage.type.sql",
            "settings": {
              "foreground": "#569CD6"
            }
          }
        ]
      }
    }
  }
}
